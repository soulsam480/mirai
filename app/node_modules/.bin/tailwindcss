#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sambit/projects/personal/mirai/node_modules/.pnpm/tailwindcss@3.0.23_b89136460714832cdda11d1e9d57d1ff/node_modules/tailwindcss/lib/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/tailwindcss@3.0.23_b89136460714832cdda11d1e9d57d1ff/node_modules/tailwindcss/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/tailwindcss@3.0.23_b89136460714832cdda11d1e9d57d1ff/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/node_modules:/home/sambit/projects/personal/mirai/node_modules:/home/sambit/projects/personal/node_modules:/home/sambit/projects/node_modules:/home/sambit/node_modules:/home/node_modules:/node_modules:/home/sambit/projects/personal/mirai/app/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/sambit/projects/personal/mirai/node_modules/.pnpm/tailwindcss@3.0.23_b89136460714832cdda11d1e9d57d1ff/node_modules/tailwindcss/lib/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/tailwindcss@3.0.23_b89136460714832cdda11d1e9d57d1ff/node_modules/tailwindcss/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/tailwindcss@3.0.23_b89136460714832cdda11d1e9d57d1ff/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/node_modules:/home/sambit/projects/personal/mirai/node_modules:/home/sambit/projects/personal/node_modules:/home/sambit/projects/node_modules:/home/sambit/node_modules:/home/node_modules:/node_modules:/home/sambit/projects/personal/mirai/app/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/tailwindcss@3.0.23_b89136460714832cdda11d1e9d57d1ff/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/tailwindcss@3.0.23_b89136460714832cdda11d1e9d57d1ff/node_modules/tailwindcss/lib/cli.js" "$@"
fi
