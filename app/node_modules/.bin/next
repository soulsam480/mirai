#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/sambit/projects/personal/mirai/node_modules/.pnpm/next@12.1.0_770cdee6eac9f5b17619eec686780a53/node_modules/next/dist/bin/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/next@12.1.0_770cdee6eac9f5b17619eec686780a53/node_modules/next/dist/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/next@12.1.0_770cdee6eac9f5b17619eec686780a53/node_modules/next/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/next@12.1.0_770cdee6eac9f5b17619eec686780a53/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/node_modules:/home/sambit/projects/personal/mirai/node_modules:/home/sambit/projects/personal/node_modules:/home/sambit/projects/node_modules:/home/sambit/node_modules:/home/node_modules:/node_modules:/home/sambit/projects/personal/mirai/app/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/sambit/projects/personal/mirai/node_modules/.pnpm/next@12.1.0_770cdee6eac9f5b17619eec686780a53/node_modules/next/dist/bin/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/next@12.1.0_770cdee6eac9f5b17619eec686780a53/node_modules/next/dist/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/next@12.1.0_770cdee6eac9f5b17619eec686780a53/node_modules/next/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/next@12.1.0_770cdee6eac9f5b17619eec686780a53/node_modules:/home/sambit/projects/personal/mirai/node_modules/.pnpm/node_modules:/home/sambit/projects/personal/mirai/node_modules:/home/sambit/projects/personal/node_modules:/home/sambit/projects/node_modules:/home/sambit/node_modules:/home/node_modules:/node_modules:/home/sambit/projects/personal/mirai/app/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/next@12.1.0_770cdee6eac9f5b17619eec686780a53/node_modules/next/dist/bin/next" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/next@12.1.0_770cdee6eac9f5b17619eec686780a53/node_modules/next/dist/bin/next" "$@"
fi
