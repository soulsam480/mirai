    "@trpc/next": "^9.20.1",
    "@trpc/react": "^9.20.1",
    "@trpc/server": "^9.20.1",
    "bcrypt": "^5.0.1",
    "clsx": "^1.1.1",
    "daisyui": "^2.3.6",
    "dayjs": "^1.10.8",
    "jotai": "^1.6.0",
    "nanoid": "^3.3.1",
    "next": "^12.1.0",
    "next-auth": "^4.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "react-query": "^3.34.16",
    "superjson": "^1.8.1",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@iconify-json/la": "^1.1.1",
    "@svgr/core": "^6.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "autoprefixer": "^10.4.2",
