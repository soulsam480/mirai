    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "next-superjson": "^0.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.10.0",
    "sass": "^1.49.9",
