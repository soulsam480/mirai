    "@trpc/react": "^9.18.0",
    "@trpc/server": "^9.18.0",
    "bcrypt": "^5.0.1",
    "clsx": "^1.1.1",
    "daisyui": "^1.25.4",
    "dayjs": "^1.10.7",
    "jotai": "^1.5.3",
    "nanoid": "^3.2.0",
    "next": "^12.0.9",
    "next-auth": "^4.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.25.3",
    "react-query": "^3.34.12",
    "superjson": "^1.7.4",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@iconify-json/la": "^1.0.1",
    "@svgr/core": "^6.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.26.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
