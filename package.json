{
  "name": "mirai",
  "version": "0.0.0",
  "description": "University placement platform",
  "scripts": {
    "lint": "eslint **/{src,prisma}/**/*.{js,ts,tsx}",
    "lint-fix": "yarn lint --fix",
    "fmt": "prettier --write **/{src,prisma}/**/*.{js,ts,tsx}",
    "dev:app": "pnpm run --filter @mirai/app dx",
    "dev:api": "pnpm run --filter @mirai/api dx",
    "dev": "concurrently --kill-others-on-fail \"pnpm run dev:app\"  \"pnpm run dev:api\"",
    "db-nuke": "pnpm run --filter @mirai/api db-nuke"
  },
  "dependencies": {
    "zod": "^3.14.2",
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "@trpc/client": "^9.20.3",
    "@prisma/client": "^3.11.0",
    "@mirai/api": "workspace:*",
    "@mirai/app": "workspace:*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "prettier": "^2.6.0",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.7.0",
    "typescript": "4.6.2",
    "@types/node": "^17.0.23",
    "concurrently": "^7.0.0",
    "nanoid": "^3.3.1"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com-soulsam480:soulsam480/mirai.git"
  }
}
