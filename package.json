{
  "name": "mirai",
  "version": "0.0.0",
  "private": true,
  "license": "GNU GPLv3",
  "scripts": {
    "build:1-migrate": "prisma migrate deploy",
    "build:2-build": "next build",
    "build": "run-s build:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "next dev",
    "dx": "cross-env DATABASE_URL=postgresql://postgres:@localhost:5632/mirai run-s clear db-up db-migrate-dev db-seed dev",
    "start": "next start",
    "lint": "eslint src --no-cache",
    "lint-fix": "yarn lint --fix",
    "ts-node": "ts-node -T --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "postinstall": "prisma generate",
    "fmt": "prettier --write {src,prisma}/**/*.{js,ts,tsx}",
    "clear": "rimraf .next",
    "typecheck": "tsc --noEmit --pretty -p tsconfig.json --outDir tmp/tsc-output"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@hookform/resolvers": "^2.8.8",
    "@prisma/client": "^3.11.0",
    "@trpc/client": "^9.20.3",
    "@trpc/next": "^9.20.3",
    "@trpc/react": "^9.20.3",
    "@trpc/server": "^9.20.3",
    "bcrypt": "^5.0.1",
    "clsx": "^1.1.1",
    "daisyui": "^2.12.0",
    "dayjs": "^1.11.0",
    "jotai": "^1.6.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.1",
    "next": "^12.1.0",
    "next-auth": "^4.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.28.1",
    "react-query": "^3.34.16",
    "superjson": "^1.8.1",
    "zod": "^3.14.2"
  },
  "devDependencies": {
    "@iconify-json/la": "^1.1.1",
    "@svgr/core": "^6.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.4",
    "babel-plugin-superjson-next": "^0.4.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "next-superjson": "^0.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
