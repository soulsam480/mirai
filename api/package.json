{
  "name": "@mirai/api",
  "version": "0.0.0",
  "description": "",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "pnpm prisma migrate dev --schema=../prisma/schema.prisma",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "vite --force",
    "dx": "cross-env DATABASE_URL=postgresql://postgres:@localhost:5632/mirai run-s db-up db-migrate-dev db-seed dev",
    "ts-node": "ts-node -T --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "build-1": "prisma migrate deploy --schema=../prisma/schema.prisma",
    "build-2": "cross-env NODE_ENV=production vite build",
    "copy-prisma": "node copy.js",
    "build": "run-s build-* copy-prisma",
    "postinstall": "prisma generate --schema=../prisma/schema.prisma",
    "start": "cross-env NODE_ENV=production node dist/app.js"
  },
  "prisma": {
    "seed": "pnpm ts-node ../prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@trpc/server": "^9.22.0",
    "dotenv": "^16.0.0",
    "events": "^3.3.0",
    "fastify": "^3.28.0",
    "pino": "^7.10.0",
    "pino-pretty": "^7.6.1",
    "prisma": "^3.12.0",
    "prisma-exclude": "^1.0.2",
    "superjson": "^1.8.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@mirai/app": "workspace:*",
    "@swc/core": "*",
    "vite": "^2.9.5",
    "vite-plugin-node": "^0.0.19"
  }
}
