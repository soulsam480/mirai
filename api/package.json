{
  "name": "@mirai/api",
  "version": "0.0.0",
  "description": "",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev --schema=../prisma/schema.prisma",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "ts-node-dev --respawn --transpile-only --rs --pretty --watch src ./src/app.ts",
    "dx": "cross-env DATABASE_URL=postgresql://postgres:@localhost:5632/mirai run-s db-up db-migrate-dev db-seed dev",
    "ts-node": "ts-node -T --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "build-1": "prisma migrate deploy --schema=../prisma/schema.prisma",
    "build-2": "rimraf dist && tsup",
    "copy-prisma": "node copy.js",
    "build": "run-s build-* copy-prisma",
    "postinstall": "prisma generate --schema=../prisma/schema.prisma",
    "start": "node dist/index.js"
  },
  "prisma": {
    "seed": "yarn ts-node ../prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@trpc/server": "^9.20.3",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.10.0",
    "fastify-cli": "^2.15.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.2",
    "prisma": "^3.11.0",
    "prisma-exclude": "^1.0.1",
    "superjson": "^1.8.1",
    "tsconfig-paths": "^3.14.1"
  },
  "devDependencies": {
    "@mirai/app": "workspace:*",
    "@types/bcrypt": "^5.0.0",
    "ts-node-dev": "^1.1.8",
    "tsup": "^5.12.1"
  }
}
