{
  "name": "@mirai/api",
  "version": "0.0.0",
  "description": "",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "pnpm prisma migrate dev --schema=../prisma/schema.prisma",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "cross-env NODE_ENV=development ts-node-dev --rs --transpile-only --exit-child --ignore-watch node_modules --watch src -- src/app.ts",
    "dx": "cross-env DATABASE_URL=postgresql://postgres:@localhost:5632/mirai run-s db-up db-migrate-dev db-seed dev",
    "ts-node": "ts-node -T --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "start-1": "prisma migrate deploy --schema=../prisma/schema.prisma",
    "start-2": "cross-env NODE_ENV=production ts-node --pretty -T src/app.ts",
    "postinstall": "prisma generate --schema=../prisma/schema.prisma",
    "start": "run-s start-*",
    "studio": "cross-env DATABASE_URL=postgresql://postgres:@localhost:5632/mirai prisma studio --schema=../prisma/schema.prisma"
  },
  "prisma": {
    "seed": "pnpm ts-node ../prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@bull-board/api": "^4.3.1",
    "@bull-board/fastify": "^4.3.1",
    "@fastify/autoload": "^5.4.0",
    "@fastify/cookie": "^8.1.0",
    "@fastify/cors": "^8.1.0",
    "@fastify/websocket": "^7.0.1",
    "@swc/helpers": "^0.4.11",
    "@trpc/server": "^9.27.1",
    "@typegoose/typegoose": "^9.12.0",
    "bullmq": "^1.91.0",
    "dotenv": "^16.0.2",
    "fastify": "^4.6.0",
    "fastify-plugin": "^4.2.1",
    "mongoose": "~6.6.1",
    "next-auth": "*",
    "pino": "^8.5.0",
    "pino-pretty": "^9.1.0",
    "prisma": "^4.3.1",
    "prisma-exclude": "^1.0.2",
    "superjson": "^1.10.0",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@mirai/app": "workspace:*",
    "@swc/core": "*",
    "@types/bull": "^3.15.9",
    "@types/ws": "^8.5.3",
    "typescript": "*"
  }
}
