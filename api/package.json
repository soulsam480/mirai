{
  "name": "@mirai/api",
  "version": "0.0.0",
  "description": "",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "pnpm prisma migrate dev --schema=../prisma/schema.prisma",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "cross-env NODE_ENV=development ts-node-dev --rs --transpile-only --exit-child --ignore-watch node_modules --watch src -- src/app.ts",
    "dx": "cross-env DATABASE_URL=postgresql://postgres:@localhost:5632/mirai run-s db-up db-migrate-dev db-seed dev",
    "ts-node": "ts-node -T --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "build-1": "prisma migrate deploy --schema=../prisma/schema.prisma",
    "build-2": "cross-env NODE_ENV=production vite build",
    "copy-prisma": "node copy.js",
    "build": "run-s build-* copy-prisma",
    "postinstall": "prisma generate --schema=../prisma/schema.prisma",
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "studio": "cross-env DATABASE_URL=postgresql://postgres:@localhost:5632/mirai prisma studio --schema=../prisma/schema.prisma"
  },
  "prisma": {
    "seed": "pnpm ts-node ../prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@bull-board/api": "^3.11.0",
    "@bull-board/fastify": "^3.11.0",
    "@fastify/autoload": "^4.0.1",
    "@fastify/cookie": "^6.0.0",
    "@fastify/cors": "^7.0.0",
    "@fastify/websocket": "^5.0.0",
    "@swc/helpers": "^0.3.17",
    "@trpc/server": "^9.23.6",
    "@typegoose/typegoose": "^9.9.0",
    "bull": "^4.8.3",
    "bullmq": "^1.86.0",
    "dotenv": "^16.0.1",
    "fastify": "^3.29.0",
    "fastify-plugin": "^3.0.1",
    "mongoose": "^6.3.6",
    "next-auth": "*",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "prisma": "^3.14.0",
    "prisma-exclude": "^1.0.2",
    "superjson": "^1.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@mirai/app": "workspace:*",
    "@swc/core": "*",
    "@types/bull": "^3.15.8",
    "@types/ws": "^8.5.3",
    "typescript": "*"
  }
}
